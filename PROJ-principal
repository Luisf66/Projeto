//  Assinatura das funções
void menu(void); //v
void info(void); //v
void equipe(void); //v
void paciente(void); //v
void profissionais(void); //v
void servicos(void); //v
void pagamento(void); //v
void horario(void); //v 
void cadastrarpaciente(void); //v
void cadastrarprofissional(void); //v
