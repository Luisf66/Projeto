void agendar(void); //v
void remarcar(void); //v 
void cancelar(void); //v
void acesso_online(void); //v


//  Programa principal
int main(void) {
    menu();
    info();
    equipe();
    paciente();
    profissionais();
    servicos();
    pagamento();
    horario();
    cadastrarpaciente(); 
    cadastrarprofissional(); 
    editarpaciente(); 
    editarprofissional(); 
    deletarpaciente(); 
    deletarprofissional(); 
    agendamento();
    agendar();
    remarcar();
    cancelar();
    acesso_online();
    return 0;
}

int dataValida(int, int, int);
